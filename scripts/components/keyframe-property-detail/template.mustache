<h1 class="$propertyName keyframe-property-name">Detail</h1>
<div class="add-delete-wrapper">
  <button class="icon-button add" title="Add a new keyframe to the current track">
    <i class="glyphicon glyphicon-plus"></i>
  </button>
  <button class="icon-button delete" title="Remove the currently selected keyframe">
    <i class="glyphicon glyphicon-minus"></i>
  </button>
</div>
<label class="label-input-pair row keyframe-property-millisecond">
  <p>Millisecond:</p>
  <input class="$propertyMillisecond property-millisecond" type="number" value="" name="millisecond" min="0">
</label>
<label class="label-input-pair row keyframe-property-value">
  <p>Value:</p>
  <input class="$propertyValue property-value" type="text" value="" name="value">
</label>
<label class="label-input-pair row select-container keyframe-property-easing">
  <p>Easing:</p>
  <select class="$propertyEasing" name="easing"></select>
</label>
